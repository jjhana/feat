<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE MIME-resolver PUBLIC "-//NetBeans//DTD MIME Resolver 1.0//EN" "http://www.netbeans.org/dtds/mime-resolver-1_0.dtd">
<MIME-resolver>
    <file>
        <ext name="cmd"/>
        <resolver mime="text/x-speedcmd">
            <xml-rule>
                <element name="FeatCommandFile"/>
            </xml-rule>
        </resolver>
    </file>
    <file>
        <ext name="xml"/>
        <resolver mime="text/x-speedcmd">
            <xml-rule>
                <element name="FeatCommandFile"/>
            </xml-rule>
        </resolver>
    </file>
</MIME-resolver>
