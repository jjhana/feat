<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE filesystem PUBLIC "-//NetBeans//DTD Filesystem 1.2//EN" "http://www.netbeans.org/dtds/filesystem-1_2.dtd">
<filesystem>
    <folder name="Services">
        <folder name="AutoupdateType">
            <!-- show feat's normal, beta and test UC and NB's main UC -->

            <!-- old UC, no longer used -->
            <file name="beta-update-provider.instance_hidden"/>
            <file name="pluginportal-update-provider.instance_hidden"/>
            <file name="thirdparty-update-provider.instance_hidden"/>
            <file name="cz_cuni_utkl_czesl_main_update_center.instance_hidden"/>
            <file name="cz_cuni_utkl_czesl_main_update_center_beta.instance_hidden"/>

            <!-- hide nb_modules UC as it is now included in the feat's UC; keep NB's UC declared in nb_modules as well -->
            <file name="org_purl_net_jh_nbutil_update_center.instance_hidden"/>
            <file name="org_purl_net_jh_nbutil_beta_update_center.instance_hidden"/>

            <file name="feat_update_center.instance">
                <attr name="displayName" bundlevalue="org.purl.net.jh.feat.brand.Bundle#Services/AutoupdateType/feat_update_center.instance"/>
                <attr name="enabled" boolvalue="true"/>
                <attr name="instanceCreate" methodvalue="org.netbeans.modules.autoupdate.updateprovider.AutoupdateCatalogFactory.createUpdateProvider"/>
                <attr name="instanceOf" stringvalue="org.netbeans.spi.autoupdate.UpdateProvider"/>
                <attr name="url" bundlevalue="org.purl.net.jh.feat.brand.Bundle#feat_update_center"/>
            </file>
            <file name="feat_beta_update_center.instance">
                <attr name="displayName" bundlevalue="org.purl.net.jh.feat.brand.Bundle#Services/AutoupdateType/feat_beta_update_center.instance"/>
                <attr name="enabled" boolvalue="false"/>
                <attr name="instanceCreate" methodvalue="org.netbeans.modules.autoupdate.updateprovider.AutoupdateCatalogFactory.createUpdateProvider"/>
                <attr name="instanceOf" stringvalue="org.netbeans.spi.autoupdate.UpdateProvider"/>
                <attr name="url" bundlevalue="org.purl.net.jh.feat.brand.Bundle#feat_beta_update_center"/>
            </file>
            <file name="feat_test_update_center.instance">
                <attr name="displayName" bundlevalue="org.purl.net.jh.feat.brand.Bundle#Services/AutoupdateType/feat_test_update_center.instance"/>
                <attr name="enabled" boolvalue="false"/>
                <attr name="instanceCreate" methodvalue="org.netbeans.modules.autoupdate.updateprovider.AutoupdateCatalogFactory.createUpdateProvider"/>
                <attr name="instanceOf" stringvalue="org.netbeans.spi.autoupdate.UpdateProvider"/>
                <attr name="url" bundlevalue="org.purl.net.jh.feat.brand.Bundle#feat_test_update_center"/>
            </file>
        </folder>
    </folder>
</filesystem>
